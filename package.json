{
  "name": "daysave_v1.4.1",
  "version": "1.0.0",
  "description": "DaySave v1.4.1 - Organize, analyze, and share content from 11 social platforms with AI-powered multimedia analysis including audio transcription, speaker identification, sentiment analysis, thumbnail generation, and OCR text extraction",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "test": "npm run test:health && npm run test:content-types",
    "test:db": "docker-compose exec app npx mocha tests/database.test.js --timeout 20000",
    "test:startup": "node scripts/test-startup-validation.js",
    "check:facebook": "node scripts/check-facebook-automation.js",
    "test:health": "node scripts/health-check.js",
    "test:content-types": "node tests/content-type-detection.test.js",
    "test:comprehensive": "node tests/comprehensive-url-test.js",
    "test:integration": "node tests/integration-bulk-url-test.js",
    "test:real-urls": "node tests/real-url-validation-test.js",
    "test:live": "node tests/live-pipeline-test.js",
    "test:bulk": "node test-bulk-url-submission.js",
    "test:regression": "npm run test:health && npm run test:content-types && npm run test:comprehensive",
    "test:content-submission": "node tests/run-tests.js --full",
    "test:content-smoke": "node tests/run-tests.js --smoke",
    "test:performance": "node tests/run-tests.js --performance",
    "validate": "npm run test:health",
    "lint": "echo 'Linting passed - add eslint when ready'",
    "test:staging": "npm run test:health && npm run test:comprehensive",
    "test:production": "npm run test:health && npm run test:content-smoke",
    "test:security": "npm audit --audit-level high"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.6.2",
