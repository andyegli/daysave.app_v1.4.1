    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/vision": "^4.0.2",
    "@google/generative-ai": "^0.24.1",
    "@sendgrid/mail": "^8.1.3",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.0",
    "compression": "^1.8.0",
    "connect-mysql-session": "^0.1.1",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-rate-limit": "^7.3.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^7.1.0",
    "imap-simple": "^1.6.3",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.1",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.4",
